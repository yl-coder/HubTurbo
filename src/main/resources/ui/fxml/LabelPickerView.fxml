<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="mainLayout" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <children>
        <Label fx:id="title" maxWidth="400.0" style="-fx-font-size: 125%;" text="Issue"/>
        <FlowPane fx:id="assignedLabels" hgap="5.0" vgap="5.0">
            <padding>
                <Insets bottom="10.0" top="20.0"/>
            </padding>
        </FlowPane>
        <TextField fx:id="queryField" prefColumnCount="30"/>
        <VBox fx:id="feedbackLabels" prefHeight="200.0" prefWidth="100.0">
            <VBox.margin>
                <Insets/>
            </VBox.margin>
            <padding>
                <Insets top="10.0"/>
            </padding>
        </VBox>
    </children>
</VBox>
